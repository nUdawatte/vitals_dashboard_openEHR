#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/@sveltejs+kit@2.20.8_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.28.2_vite@6.3.5_jiti@2_8cfa6e709ad1000f763434a7f7f9b155/node_modules/@sveltejs/kit/node_modules:/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/@sveltejs+kit@2.20.8_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.28.2_vite@6.3.5_jiti@2_8cfa6e709ad1000f763434a7f7f9b155/node_modules/@sveltejs/node_modules:/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/@sveltejs+kit@2.20.8_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.28.2_vite@6.3.5_jiti@2_8cfa6e709ad1000f763434a7f7f9b155/node_modules:/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/@sveltejs+kit@2.20.8_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.28.2_vite@6.3.5_jiti@2_8cfa6e709ad1000f763434a7f7f9b155/node_modules/@sveltejs/kit/node_modules:/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/@sveltejs+kit@2.20.8_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.28.2_vite@6.3.5_jiti@2_8cfa6e709ad1000f763434a7f7f9b155/node_modules/@sveltejs/node_modules:/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/@sveltejs+kit@2.20.8_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.28.2_vite@6.3.5_jiti@2_8cfa6e709ad1000f763434a7f7f9b155/node_modules:/home/nuwan/medblocks/openehr/openehr-bootcamp/nuwan_vitals_app_v1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
